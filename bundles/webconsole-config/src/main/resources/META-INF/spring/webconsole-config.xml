<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:blueprint="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                        http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

    <blueprint:service interface="org.apache.felix.webconsole.WebConsoleSecurityProvider">
        <bean class="org.jahia.bundles.webconsole.security.DelegateWebConsoleSecurityProvider">
            <property name="requiredRole" value="toolManager" />
        </bean>
    </blueprint:service>
    
</beans>