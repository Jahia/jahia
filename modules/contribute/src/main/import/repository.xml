<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <contribute j:dependencies="contentRetrieval default default-skins facets grid skins tabularList tags"
                          j:installedModules="contribute"
                  j:modulePriority="0"
                          j:moduleType="module"
                          j:title="Jahia Contribute Mode"
                          jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder">
                            <logo-jahia.png j:height="30"
                                            j:width="57"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </logo-jahia.png>
                        </default>
                    </themes>
                    <icons jcr:primaryType="jnt:folder">
                        <my-settings.png j:height="64"
                                         j:width="64"
                                         jcr:mixinTypes="jmix:image"
                                         jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-settings.png>
                        <my-pages.png j:height="64"
                                      j:width="64"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-pages.png>
                        <my-sites.png j:height="64"
                                      j:width="64"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-sites.png>
                        <myprofile.png j:height="48"
                                       j:width="48"
                                       jcr:mixinTypes="jmix:image"
                                       jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </myprofile.png>
                        <mybookmarks.png j:height="48"
                                         j:width="48"
                                         jcr:mixinTypes="jmix:image"
                                         jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mybookmarks.png>
                        <mysites.png j:height="48"
                                     j:width="48"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mysites.png>
                        <mycontent.png j:height="48"
                                       j:width="48"
                                       jcr:mixinTypes="jmix:image"
                                       jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mycontent.png>
                        <view-content.png j:height="48"
                                          j:width="48"
                                          jcr:mixinTypes="jmix:image"
                                          jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </view-content.png>
                        <folder-content.png j:height="48"
                                            j:width="48"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </folder-content.png>
                        <edit-content.png j:height="48"
                                          j:width="48"
                                          jcr:mixinTypes="jmix:image"
                                          jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </edit-content.png>
                        <folder-files.png j:height="48"
                                          j:width="48"
                                          jcr:mixinTypes="jmix:image"
                                          jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </folder-files.png>
                        <folder-content-files.png j:height="64"
                                                  j:width="81"
                                                  jcr:mixinTypes="jmix:image"
                                                  jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </folder-content-files.png>
                        <myspace.png j:height="48"
                                     j:width="48"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </myspace.png>
                    </icons>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base j:view="contribute"
                      jcr:mixinTypes="jmix:theme"
                      jcr:primaryType="jnt:template">
                    <viewContent j:applyOn="jnt:content"
                                 j:hiddenTemplate="true"
                                 j:priority="10"
                                 jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <custom-rows-960gs customColumn="16" divID="content" jcr:primaryType="jnt:customRow">
                                <custom-rows-960gs-col1 jcr:primaryType="jnt:contentList">
                                        <view-content j:skin="skins.box8"
                                                  j:style="blue"
                                                  jcr:mixinTypes="jmix:skinnable jmix:style"
                                                  jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.viewContent)##"/>
                                        <back-to-parent-page jcr:mixinTypes="jmix:accessControlled"
                                                             jcr:primaryType="jnt:backToParentPage">
                                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                        </back-to-parent-page>
                                        <mainResourceDisplay j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                                    </view-content>
                                </custom-rows-960gs-col1>
                            </custom-rows-960gs>
                        </pagecontent>
                    </viewContent>
                    <fileTemplate j:applyOn="jnt:file"
                                  j:defaultTemplate="true"
                                  j:priority="10"
                                  jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <custom-rows-960gs customColumn="16" divID="content" jcr:primaryType="jnt:customRow">
                                <custom-rows-960gs-col1 jcr:primaryType="jnt:contentList">
                                    <view-document j:skin="skins.box8"
                                                   j:style="blue"
                                                   jcr:mixinTypes="jmix:skinnable jmix:style"
                                                   jcr:primaryType="jnt:contentList">
                                        <back-to-parent-page jcr:mixinTypes="jmix:accessControlled"
                                                             jcr:primaryType="jnt:backToParentPage">
                                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                        </back-to-parent-page>
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.viewDocument)##"/>
                                        <mainResourceDisplay j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                                    </view-document>
                                </custom-rows-960gs-col1>
                            </custom-rows-960gs>
                        </pagecontent>
                    </fileTemplate>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <content jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="##resourceBundle(system.content)##"/>
                                <pagecontent jcr:primaryType="jnt:area"/>
                        </content>
                        <footer colNames="footer-colContent"
                                customColumn="16"
                                divID="footer"
                                jcr:primaryType="jnt:customRow">
                            <footer-colContent jcr:primaryType="jnt:contentList">
                                <copyright jcr:primaryType="jnt:bigText">
                                    <j:translation_en jcr:language="en"
                                                      jcr:primaryType="jnt:translation"
                                                      text="&lt;p style=&#34;text-align: center;&#34;&gt;  &amp;copy; 2002-2014 Jahia Solutions Group SA - ##resourceBundle(org.jahia.copyright)##&lt;/p&gt; "/>
                                </copyright>
                            </footer-colContent>
                        </footer>
                    </pagecontent>
                    <contentFolderTemplate j:applyOn="jnt:contentFolder"
                                           j:defaultTemplate="true"
                                           j:hiddenTemplate="false"
                                           j:priority="11"
                                           jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <custom-rows-960gs-1 customColumn="3,13" divID="content" jcr:primaryType="jnt:customRow">
                                <custom-rows-960gs-1-col1 jcr:primaryType="jnt:contentList">
                                    <facets j:skin="skins.box8"
                                            jcr:mixinTypes="jmix:skinnable jmix:style"
                                            jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.facets)##"/>
                                        <facets j:type="jnt:content" jcr:primaryType="jnt:facets">
                                            <fieldFacet facet="tagList"
                                                        field="jmix:tagged;j:tagList"
                                                        jcr:primaryType="jnt:fieldFacet"
                                                        limit="100"
                                                        mincount="1"
                                                        missing="false"
                                                        offset="0">
                                                <j:translation_en jcr:language="en"
                                                                  jcr:primaryType="jnt:translation"
                                                                  label="##resourceBundle(label.tags)##"/>
                                            </fieldFacet>
                                            <field-facet facet="categories"
                                                         field="jmix:categorized;j:defaultCategory"
                                                         jcr:primaryType="jnt:fieldFacet"
                                                         limit="100"
                                                         mincount="1"
                                                         missing="false"
                                                         offset="0"
                                                         sort="index">
                                                <j:translation_en jcr:language="en"
                                                                  jcr:primaryType="jnt:translation"
                                                                  label="##resourceBundle(label.categories)##"/>
                                            </field-facet>
                                        </facets>
                                    </facets>
                                </custom-rows-960gs-1-col1>
                                <custom-rows-960gs-1-col2 jcr:primaryType="jnt:contentList">
                                        <content-folder j:skin="skins.box8"
                                                    j:style="blue"
                                                    jcr:mixinTypes="jmix:skinnable jmix:style"
                                                    jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(content.folder)##"/>
                                        <pager displayNbOfItemsPerPage="true"
                                               displayPager="true"
                                               jcr:primaryType="jnt:pager"
                                               nbOfPages="10"
                                               pageSize="10"/>
                                        <mainResourceDisplay j:canDeleteInContribution="true"
                                                             j:canOrderInContribution="true"
                                                             j:contributeTypes="jmix:editorialContent jnt:contentFolder"
                                                             j:editableInContribution="true"
                                                             j:mainResourceView="table"
                                                             jcr:mixinTypes="jmix:contributeMode"
                                                             jcr:primaryType="jnt:mainResourceDisplay"/>
                                    </content-folder>
                                </custom-rows-960gs-1-col2>
                            </custom-rows-960gs-1>
                        </pagecontent>
                    </contentFolderTemplate>
                    <documentFolderTemplate j:applyOn="jnt:folder"
                                            j:defaultTemplate="true"
                                            j:hiddenTemplate="false"
                                            j:priority="11"
                                            jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <custom-rows-960gs-1 customColumn="3,13" divID="content" jcr:primaryType="jnt:customRow">
                                <custom-rows-960gs-1-col1 jcr:primaryType="jnt:contentList">
                                    <facets j:skin="skins.box8"
                                            jcr:mixinTypes="jmix:skinnable jmix:style"
                                            jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.facets)##"/>
                                        <facets j:type="jnt:file" jcr:primaryType="jnt:facets">
                                            <fieldFacet facet="tagLib"
                                                        field="jmix:tagged;j:tagList"
                                                        jcr:primaryType="jnt:fieldFacet"
                                                        limit="100"
                                                        mincount="1"
                                                        missing="false"
                                                        offset="0">
                                                <j:translation_en jcr:language="en"
                                                                  jcr:primaryType="jnt:translation"
                                                                  label="##resourceBundle(label.tags)##"/>
                                            </fieldFacet>
                                            <field-facet facet="categories"
                                                         field="jmix:categorized;j:defaultCategory"
                                                         jcr:primaryType="jnt:fieldFacet"
                                                         limit="100"
                                                         mincount="1"
                                                         missing="false"
                                                         offset="0"
                                                         sort="index">
                                                <j:translation_en jcr:language="en"
                                                                  jcr:primaryType="jnt:translation"
                                                                  label="##resourceBundle(label.categories)##"/>
                                            </field-facet>
                                        </facets>
                                    </facets>
                                </custom-rows-960gs-1-col1>
                                <custom-rows-960gs-1-col2 jcr:primaryType="jnt:contentList">
                                        <document-folder j:skin="skins.box8"
                                                     j:style="blue"
                                                     jcr:mixinTypes="jmix:skinnable jmix:style"
                                                     jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.documentFolder)##"/>
                                        <pager displayNbOfItemsPerPage="true"
                                               displayPager="true"
                                               jcr:primaryType="jnt:pager"
                                               nbOfPages="10"
                                               pageSize="10"/>
                                        <mainResourceDisplay j:canDeleteInContribution="true"
                                                             j:canOrderInContribution="true"
                                                             j:contributeTypes="jmix:editorialContent jnt:contentFolder"
                                                             j:editableInContribution="true"
                                                             j:mainResourceView="table"
                                                             jcr:mixinTypes="jmix:contributeMode"
                                                             jcr:primaryType="jnt:mainResourceDisplay"/>
                                    </document-folder>
                                </custom-rows-960gs-1-col2>
                            </custom-rows-960gs-1>
                        </pagecontent>
                    </documentFolderTemplate>
                </base>
            </templates>
                
        </contribute>
    </modules>
</content>
