<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahia.modules.portal.CloneAction">
        <property name="name" value="clone"/>
        <property name="contentManager" ref="ContentManagerHelper"/>
    </bean>

    <bean class="org.jahia.modules.portal.CreatePortal">
        <property name="name" value="createPortal"/>
    </bean>
</beans>