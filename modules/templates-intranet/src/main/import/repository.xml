                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Home" jcr:primaryType="jnt:translation" />
                <j:translation_de jcr:language="de" jcr:mixinTypes="mix:title" jcr:title="Start" jcr:primaryType="jnt:translation" />
                <logo jcr:primaryType="jnt:contentList" />
                <listA jcr:primaryType="jnt:contentList" />
