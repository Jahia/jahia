                                <j:translation_en jcr:title="Login to Jahia" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation" jcr:language="en" />
                                <j:translation_de jcr:title="Jahia-Login" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation" jcr:language="de" />
                            </login-to-jahia>
                            <listB jcr:primaryType="jnt:area" />
                        </row1-col2>
                    </home>
                    <simple jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
