                        </pagecontent>
                        <row1-col1 j:skin="skins.box3style1" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listA jcr:primaryType="jnt:area" />
                        </row1-col1>
                        <row1-col2 j:skin="skins.box3style1" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listB jcr:primaryType="jnt:area" />
                        </row1-col2>
                        <row1-col3 j:skin="skins.box5" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listC jcr:primaryType="jnt:area" />
                        </row1-col3>
                    </double>
                    <events jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row1 column="2col115" jcr:primaryType="jnt:row" />
                        </pagecontent>
                        <row1-col1 j:skin="skins.box3style1" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listA jcr:primaryType="jnt:area" />
                            <events jcr:primaryType="jnt:area" j:allowedTypes="jnt:event" />
                        </row1-col1>
                        <row1-col2 j:skin="skins.box5" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <calendar startDateProperty="startDate" endDateProperty="endDate" jcr:primaryType="jnt:calendar" j:bindedComponent="/templateSets/templates-intranet/templates/base/events/row1-col1/events" />
                            <facets jcr:primaryType="jnt:facets" j:bindedComponent="/templateSets/templates-intranet/templates/base/events/row1-col1/events">
                                <fieldFacet mincount="1" field="jnt:event;eventsType" jcr:primaryType="jnt:fieldFacet" missing="false" facet="type" offset="0" limit="100">
                                    <j:translation_en label="Type" jcr:language="en" jcr:primaryType="jnt:translation" />
                                    <j:translation_de label="Typ" jcr:language="de" jcr:primaryType="jnt:translation" />
                                </fieldFacet>
                            </facets>
                        </row1-col2>
                    </events>
                    <summary jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="2col115" jcr:primaryType="jnt:row" />
                        </pagecontent>
                        <row-col1 j:skin="skins.box3style1" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <summary jcr:primaryType="jnt:summary" j:bindedComponent="/templateSets/templates-intranet/templates/base/summary/row-col1/listA">
                                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Summary" jcr:primaryType="jnt:translation" />
                                <j:translation_de jcr:language="de" jcr:mixinTypes="mix:title" jcr:title="Zusammenfassung" jcr:primaryType="jnt:translation" />
                            </summary>
                            <listA jcr:primaryType="jnt:area" j:areaView="anchor" j:allowedTypes="" />
                        </row-col1>
                        <row-col2 j:skin="skins.box5" jcr:mixinTypes="jmix:style jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listB jcr:primaryType="jnt:area" j:allowedTypes="" />
                        </row-col2>
                    </summary>
                    <contentTemplate jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:content" j:hiddenTemplate="false" j:defaultTemplate="true" j:priority="1">
                      <pagecontent jcr:primaryType="jnt:contentList">
                        <mainResourceDisplay jcr:primaryType="jnt:mainResourceDisplay" />
                      </pagecontent>
                    </contentTemplate>
                    <user j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false" j:priority="10" jcr:primaryType="jnt:contentTemplate">
                      <pagecontent j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable" jcr:primaryType="jnt:contentList">
                        <userDetails j:bindedComponent="/templateSets/profile/templates/user" j:view="full" jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:userDetails" />
                      </pagecontent>
