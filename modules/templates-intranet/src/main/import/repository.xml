                        </row1-col1>
                        <row1-col2 j:skin="skins.box5" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
                            <listB jcr:primaryType="jnt:area" />
                        </row1-col2>
                    </simple>
                    <double jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
