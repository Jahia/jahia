                                        <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                        <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                    </navbar-bg.png>
                                </img>
                                <mod jcr:primaryType="jnt:folder">
                                    <shortcuts-bg-on.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="38" j:width="102">
