                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder" />
                        <theme-yellow jcr:primaryType="jnt:folder">
                            <theme-yellow jcr:primaryType="jnt:folder">
                                <img jcr:primaryType="jnt:folder">
                                    <bottomheader-bg.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="113" j:width="13">
