                                        <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                        <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                    </shortcuts-bg-on.png>
                                    <shortcuts-bg.png jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file" j:height="38" j:width="102">
                                        <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                        <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                        <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource" />
                                    </shortcuts-bg.png>
                                </mod>
                            </theme-yellow>
                            <theme-yellow.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource" />
                            </theme-yellow.css>
                        </theme-yellow>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:template" j:view="intranet">
                    <logo jcr:primaryType="jnt:contentList">
                        <logo j:basenode="/templateSets/templates-intranet/home" jcr:primaryType="jnt:absoluteArea" />
                    </logo>
                    <simpleSearch jcr:primaryType="jnt:contentList">
                        <simpleSearchForm result="/templateSets/templates-intranet/search-results" jcr:primaryType="jnt:simpleSearchForm" />
                    </simpleSearch>
                    <topMenu jcr:primaryType="jnt:contentList">
                        <top-menu j:menuType="/templateSets/templates-intranet/templates/contents/menuAvailable/primary" j:maxDepth="2" jcr:primaryType="jnt:navMenu" j:baselineNode="home" j:styleName="navigation" />
                        <shortcuts jcr:primaryType="jnt:shortcuts" />
                    </topMenu>
                    <footer jcr:primaryType="jnt:contentList">
                        <footer-row column="1col16" jcr:primaryType="jnt:row" />
                    </footer>
                    <footer-row-col1 j:skin="skins.box3style1" jcr:mixinTypes="jmix:skinnable" jcr:primaryType="jnt:contentList">
