          <footer-col1 jcr:primaryType="jnt:contentList">
            <footer-left j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </footer-col1>
          <content-colContent jcr:primaryType="jnt:contentList" />
          <header-col2 jcr:primaryType="jnt:contentList">
            <Header-right j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </header-col2>
          <footerTop-col4 jcr:primaryType="jnt:contentList">
            <topfooter4 j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </footerTop-col4>
          <footer-col2 jcr:primaryType="jnt:contentList">
            <footer-right j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </footer-col2>
          <illustration-colContent jcr:primaryType="jnt:contentList">
            <illustration j:allowedTypes="" jcr:primaryType="jnt:area" />
          </illustration-colContent>
          <events jcr:primaryType="jnt:pageTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
              <row1 column="2col115" jcr:primaryType="jnt:row" />
            </pagecontent>
            <row1-col1 jcr:primaryType="jnt:contentList">
              <pageTitle jcr:primaryType="jnt:pageTitle" />
              <breadcrumb displayCurrentPage="true" displayHome="true" displayLinkOnCurrentPage="false" jcr:primaryType="jnt:pageBreadcrumb" />
              <listA jcr:primaryType="jnt:area" />
              <events j:allowedTypes="jnt:event" j:canDeleteInContribution="true" j:canOrderInContribution="true" j:contributeTypes="jnt:event" j:editableInContribution="true" jcr:mixinTypes="jmix:contributeMode" jcr:primaryType="jnt:area" />
            </row1-col1>
            <row1-col2 jcr:primaryType="jnt:contentList">
              <side-menu j:baselineNode="home" j:maxDepth="3" j:startLevel="1" j:styleName="navigationN2" jcr:primaryType="jnt:navMenu" />
              <calendar endDateProperty="endDate" j:bindedComponent="/templateSets/templates-web-blue/templates/base/events/row1-col1/events" jcr:primaryType="jnt:calendar" startDateProperty="startDate" />
              <facets j:bindedComponent="/templateSets/templates-web-blue/templates/base/events/row1-col1/events" jcr:primaryType="jnt:facets">
                <fieldFacet facet="type" field="jnt:event;eventsType" jcr:primaryType="jnt:fieldFacet" limit="100" mincount="1" missing="false" offset="0">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Type" />
                </fieldFacet>
                <fieldFacet-1 facet="Categories" field="jmix:categorized;j:defaultCategory" jcr:primaryType="jnt:fieldFacet" limit="100" mincount="1" missing="false" offset="0">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Categories" />
                </fieldFacet-1>
                <queryFacet facet="nextEvents" jcr:primaryType="jnt:queryFacet" mincount="1" query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH]">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Next events per month" valueLabel="##DynamicDateLabel(NOW/MONTH,date,MMMM)##" />
                </queryFacet>
                <queryFacet-1 facet="nextEvents" jcr:primaryType="jnt:queryFacet" mincount="1" query="0\:FACET\:startDate:[NOW/MONTH+1MONTH TO NOW/MONTH+2MONTH] OR 0\:FACET\:endDate:[NOW/MONTH+1MONTH TO NOW/MONTH+2MONTH]">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Next events per month" valueLabel="##DynamicDateLabel(NOW/MONTH+1MONTH,date,MMMM)##" />
                </queryFacet-1>
                <queryFacet-2 facet="nextEvents" jcr:primaryType="jnt:queryFacet" mincount="1" query="0\:FACET\:startDate:[NOW/MONTH+2MONTH TO NOW/MONTH+3MONTH] OR 0\:FACET\:endDate:[NOW/MONTH+2MONTH TO NOW/MONTH+3MONTH]">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Next events per month" valueLabel="##DynamicDateLabel(NOW/MONTH+2MONTH,date,MMMM)##" />
                </queryFacet-2>
                <queryFacet-3 facet="nextEvents" jcr:primaryType="jnt:queryFacet" mincount="1" query="0\:FACET\:startDate:[NOW/MONTH+3MONTH TO NOW/MONTH+4MONTH] OR 0\:FACET\:endDate:[NOW/MONTH+3MONTH TO NOW/MONTH+4MONTH]">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Next events per month" valueLabel="##DynamicDateLabel(NOW/MONTH+3MONTH,date,MMMM)##" />
                </queryFacet-3>
              </facets>
            </row1-col2>
            <illustration jcr:primaryType="jnt:contentList">
              <illustration j:allowedTypes="jnt:imageReference" j:level="1" jcr:primaryType="jnt:levelAbsoluteArea" />
            </illustration>
          </events>
          <news-template j:applyOn="jnt:news" j:defaultTemplate="true" j:priority="100" jcr:primaryType="jnt:contentTemplate">
