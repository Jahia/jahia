      <contents jcr:primaryType="jnt:contentFolder" />
      <files jcr:primaryType="jnt:folder" />
      <portlets jcr:primaryType="jnt:portletFolder" />
      <templates jcr:primaryType="jnt:templatesFolder">
        <base j:view="templates-web-blue" jcr:mixinTypes="jmix:theme" jcr:primaryType="jnt:template">
          <home jcr:primaryType="jnt:pageTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
              <content customColumn="10,6 " jcr:primaryType="jnt:customRow" />
            </pagecontent>
            <content-col2 jcr:primaryType="jnt:contentList">
              <aside j:allowedTypes="" jcr:primaryType="jnt:area" />
            </content-col2>
            <content-col1 jcr:primaryType="jnt:contentList">
              <maincontent j:allowedTypes="" jcr:primaryType="jnt:area" />
            </content-col1>
            <illustration jcr:primaryType="jnt:contentList">
              <illustration j:allowedTypes="jnt:banner" j:areaView="easySlider-content" jcr:primaryType="jnt:area" />
            </illustration>
          </home>
          <simple jcr:primaryType="jnt:pageTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
              <content customColumn="10, 6 noprint" jcr:primaryType="jnt:customRow" />
            </pagecontent>
            <illustration jcr:primaryType="jnt:contentList">
              <illustration j:allowedTypes="jnt:imageReference" j:level="1" jcr:primaryType="jnt:levelAbsoluteArea" />
            </illustration>
            <content-col2 jcr:primaryType="jnt:contentList">
              <navMenu j:baselineNode="home" j:maxDepth="3" j:menuItemView="menuElement" j:startLevel="1" j:styleName="navigationN2-2" jcr:primaryType="jnt:navMenu" />
              <Right-column j:allowedTypes="" jcr:primaryType="jnt:area" />
            </content-col2>
            <content-col1 jcr:primaryType="jnt:contentList">
              <pagetitle jcr:primaryType="jnt:pageTitle" />
              <pageBreadcrumb displayCurrentPage="true" displayHome="true" displayLinkOnCurrentPage="false" jcr:primaryType="jnt:pageBreadcrumb" />
              <maincontent j:allowedTypes="" j:canDeleteInContribution="true" j:canOrderInContribution="true" j:contributeTypes="jnt:mainContent jnt:bigText" j:editableInContribution="true" jcr:mixinTypes="jmix:contributeMode" jcr:primaryType="jnt:area" />
            </content-col1>
          </simple>
          <pagecontent jcr:primaryType="jnt:contentList">
            <header customColumn="6,10" divID="header" jcr:primaryType="jnt:customRow" />
            <headerBottom customColumn="16" divID="headerBottom" jcr:primaryType="jnt:customRow"/>
            <illustration customColumn="16" divID="illustration" jcr:primaryType="jnt:customRow"/>
            <content-1 j:id="content" j:skin="skins.custom" jcr:mixinTypes="jmix:customSkin jmix:skinnable" jcr:primaryType="jnt:contentList">
              <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="content" />
              <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area" />
            </content-1>
            <footerTop customColumn="4,4,4,4" divClass=" noprint" divID="footerTop" jcr:primaryType="jnt:customRow" />
            <footer customColumn="8,8" divID="footer" jcr:primaryType="jnt:customRow" />
          </pagecontent>
          <headerBottom-colContent jcr:primaryType="jnt:contentList">
            <navMenu j:baselineNode="home" j:maxDepth="1" j:menuItemView="menuElement" j:startLevel="0" j:styleName="navigationN1" jcr:primaryType="jnt:navMenu" />
          </headerBottom-colContent>
          <header-col1 jcr:primaryType="jnt:contentList">
            <header-left j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </header-col1>
          <footerTop-col1 jcr:primaryType="jnt:contentList">
            <topfooter1 j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
          </footerTop-col1>
          <footerTop-col2 jcr:primaryType="jnt:contentList">
            <topfooter2 j:allowedTypes="" jcr:primaryType="jnt:absoluteArea" />
