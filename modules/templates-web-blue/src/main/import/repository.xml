              <Right-column j:allowedTypes="" jcr:primaryType="jnt:area" />
            </content-col2>
            <content-col1 jcr:primaryType="jnt:contentList">
              <pagetitle jcr:primaryType="jnt:pageTitle" />
              <pageBreadcrumb displayCurrentPage="true" displayHome="true" displayLinkOnCurrentPage="false" jcr:primaryType="jnt:pageBreadcrumb" />
              <maincontent j:allowedTypes="jnt:news" j:canDeleteInContribution="true" j:canOrderInContribution="true" j:contributeTypes="jnt:news" j:editableInContribution="true" jcr:mixinTypes="jmix:contributeMode" jcr:primaryType="jnt:area" />
            </content-col1>
          </news>
          <full-page jcr:primaryType="jnt:pageTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
              <row column="1col16" jcr:primaryType="jnt:row" />
            </pagecontent>
            <illustration jcr:primaryType="jnt:contentList">
              <illustration j:allowedTypes="jnt:imageReference" j:level="1" jcr:primaryType="jnt:levelAbsoluteArea" />
            </illustration>
            <content-col2 jcr:primaryType="jnt:contentList" />
            <content-col1 jcr:primaryType="jnt:contentList" />
            <row-col1 jcr:primaryType="jnt:contentList">
              <pagetitle jcr:primaryType="jnt:pageTitle" />
              <pageBreadcrumb displayCurrentPage="true" displayHome="true" displayLinkOnCurrentPage="false" jcr:primaryType="jnt:pageBreadcrumb" />
              <maincontent j:allowedTypes="" jcr:primaryType="jnt:area" />
            </row-col1>
          </full-page>
          <publications jcr:primaryType="jnt:pageTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
              <row1 column="2col115" jcr:primaryType="jnt:row" />
            </pagecontent>
            <row1-col1 jcr:primaryType="jnt:contentList">
              <pageTitle jcr:primaryType="jnt:pageTitle" />
              <breadcrumb displayCurrentPage="true" displayHome="true" displayLinkOnCurrentPage="false" jcr:primaryType="jnt:pageBreadcrumb" />
              <listA jcr:primaryType="jnt:area" />
              <publications j:allowedTypes="jnt:publication" jcr:primaryType="jnt:area" />
            </row1-col1>
            <row1-col2 jcr:primaryType="jnt:contentList">
              <facets j:bindedComponent="/templateSets/templates-web-blue/templates/base/publications/row1-col1/publications" jcr:primaryType="jnt:facets">
                <fieldFacet-1 facet="Categories" field="jmix:categorized;j:defaultCategory" jcr:primaryType="jnt:fieldFacet" limit="100" mincount="1" missing="false" offset="0">
                  <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Categories" />
                </fieldFacet-1>
              </facets>
            </row1-col2>
