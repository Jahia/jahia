      <home changefreq="monthly" j:templateNode="/templateSets/templates-web-blue/templates/base/home" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" priority="0.5">
        <header-col1 jcr:primaryType="jnt:contentList">
