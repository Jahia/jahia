<?xml version="1.0" encoding="UTF-8"?>
<templatesSet xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
              xmlns:jcr="http://www.jcp.org/jcr/1.0"
              xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
              xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:mixinTypes="jmix:sitemap"
              jcr:primaryType="jnt:virtualsite" changefreq="monthly" j:installedModules="templates-web" priority="0.5" j:siteType="templatesSet">

    <home jcr:mixinTypes="jmix:sitemap"
          j:displayInMenu="primary" j:isHomePage="true"
          jcr:primaryType="jnt:page" changefreq="monthly"
          j:templateNode="/templatesSet/templates/base/home" priority="0.5">
    </home>


    <portlets jcr:primaryType="jnt:portletFolder"/>
    <files jcr:primaryType="jnt:folder"/>
    <contents jcr:primaryType="jnt:contentFolder"/>
    <templates jcr:primaryType="jnt:templatesFolder">
        <files jcr:primaryType="jnt:folder">
            <themes jcr:primaryType="jnt:folder">
                <default jcr:primaryType="jnt:folder">

                </default>
            </themes>
        </files>
        <contents jcr:primaryType="jnt:contentFolder"/>

        <base jcr:mixinTypes="jmix:renderable jmix:theme" j:view="SITEKEY_PLACEHOLDER" jcr:primaryType="jnt:template" j:theme="/templatesSet/files/themes/default">
            <home jcr:primaryType="jnt:pageTemplate"/>

            <simple jcr:primaryType="jnt:pageTemplate"/>
        </base>

        <content-template j:applyOn="jnt:content" j:hiddenTemplate="true" j:priority="1" j:view="SITEKEY_PLACEHOLDER" jcr:primaryType="jnt:contentTemplate">
            <pagecontent jcr:primaryType="jnt:contentList">
                <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay" />
            </pagecontent>
        </content-template>
    </templates>

</templatesSet>
